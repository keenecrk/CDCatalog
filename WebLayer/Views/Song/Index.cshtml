@model IEnumerable<SharedModels.Models.SongDetailModel>

@{
    ViewBag.Title = "Index";
}

<h2>Songs</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SongTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CdTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Artist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DateReleased)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SongTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CdTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Artist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateReleased)
        </td>
        <td>
    </tr>
}

</table>
